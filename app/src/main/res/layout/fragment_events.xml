<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#C6269286"
>

    <ImageView
            android:layout_width="105"
            android:layout_height="75" app:srcCompat="@drawable/controller"
            android:layout_alignParentStart="true" android:layout_marginStart="-1dp"
            android:layout_alignParentTop="true" android:id="@+id/imageView2" android:layout_marginTop="65dp"
            android:background="#A86E42C0"
    />
    <ImageView
            android:layout_width="105"
            android:layout_height="75" app:srcCompat="@drawable/dumbbell"
            android:layout_alignParentStart="true" android:layout_marginStart="106dp"
            android:layout_alignParentTop="true" android:id="@+id/imageView3" android:layout_marginTop="65dp"
            android:background="#CB76D341"/>
    <ImageView
            android:layout_width="105"
            android:layout_height="75" app:srcCompat="@drawable/airplane" android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true" android:layout_marginEnd="95dp" android:id="@+id/imageView4"
            android:layout_marginTop="65dp" android:background="#98BEEF"/>
    <ImageView
            android:layout_width="95"
            android:layout_height="75" app:srcCompat="@drawable/misc" android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true" android:layout_marginEnd="-1dp" android:id="@+id/imageView5"
            android:layout_marginTop="65dp" android:background="#918F8F"/>
    <ImageButton
            android:layout_width="370dp"
            android:layout_height="170dp" app:srcCompat="@drawable/concert"
            android:layout_alignParentStart="true" android:layout_marginStart="25dp"
            android:layout_alignParentTop="true" android:id="@+id/imageButton10" android:layout_marginTop="172dp"
            android:scaleType="fitXY" android:backgroundTint="#C640AC9F"
    />
    <ImageButton
            android:layout_width="350dp"
            android:layout_height="150dp" app:srcCompat="@drawable/intility" android:layout_alignParentEnd="true"
            android:layout_marginEnd="25dp" android:layout_alignParentBottom="true" android:id="@+id/imageButton13"
            android:layout_marginBottom="229dp"
            android:scaleType="fitXY" android:background="#1E269286"/>
    <ImageButton
            android:layout_width="355dp"
            android:layout_height="152dp" app:srcCompat="@drawable/image" android:layout_alignParentEnd="true"
            android:layout_marginEnd="23dp" android:layout_alignParentBottom="true" android:id="@+id/imageButton14"
            android:layout_marginBottom="57dp"
            android:scaleType="fitXY" android:background="#4AFFFFFF"/>
</RelativeLayout>